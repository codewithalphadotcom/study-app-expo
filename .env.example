# Clerk – publishable key from the Clerk dashboard (API Keys section)
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=

# Stream – public API key from the Stream dashboard (available in Project Settings)
EXPO_PUBLIC_STREAM_API_KEY=your_stream_api_key

# Stream – secret key used by the server-side API routes to generate user tokens
# Never expose this on the client side
STREAM_SECRET_KEY=your_stream_secret_key